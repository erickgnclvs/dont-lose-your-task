{
  "manifest_version": 3,
  "name": "dont lose your task",
  "version": "0.1.0",
  "description": "extracts attemptId from outlier task pages and provides force claim actions.",
  "permissions": [
    "webRequest",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://app.outlier.ai/*/tasks*"
  ],
  "background": {
    "service_worker": "background.js" 
  },

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "web_accessible_resources": []  
} 